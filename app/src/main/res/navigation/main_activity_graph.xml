<?xml version="1.0" encoding="utf-8"?><navigation xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/main_activity_graph"    app:startDestination="@id/initFragment">    <fragment        android:id="@+id/tagsFragment"        android:name="llc.arma.checklisttag.ui.tag.TagsFragment"        android:label="TagsFragment"        tools:layout="@layout/nfc_tags_fragment">        <action            android:id="@+id/action_tagsFragment_to_scanFragment"            app:destination="@id/scanFragment" />        <action            android:id="@+id/action_tagsFragment_to_loginFragment2"            app:destination="@id/loginFragment"            app:launchSingleTop="true"            app:popUpTo="@id/tagsFragment"            app:popUpToInclusive="true" />        <action            android:id="@+id/action_tagsFragment_to_writeFragment"            app:destination="@id/writeFragment" />        <action            android:id="@+id/action_tagsFragment_to_bleFragment"            app:destination="@id/bleFragment" />        <action            android:id="@+id/action_tagsFragment_to_addNfcObjectDialog"            app:destination="@id/addNfcObjectDialog" />    </fragment>    <fragment        android:id="@+id/loginFragment"        android:name="llc.arma.checklisttag.ui.login.LoginFragment"        android:label="LoginFragment"        tools:layout="@layout/login_fragment">        <action            android:id="@+id/action_loginFragment_to_tagsFragment"            app:destination="@id/tagsFragment"            app:popUpTo="@id/loginFragment"            app:popUpToInclusive="true" />    </fragment>    <dialog        android:id="@+id/scanFragment"        android:name="llc.arma.checklisttag.ui.scan.ScanFragment"        android:label="ScanFragment"        tools:layout="@layout/scan_fragment"/>    <dialog        android:id="@+id/writeFragment"        android:name="llc.arma.checklisttag.ui.write.WriteFragment"        android:label="WriteFragment"        tools:layout="@layout/write_fragment"/>    <fragment        android:id="@+id/bleFragment"        android:name="llc.arma.checklisttag.ui.ble.BleFragment"        android:label="BleFragment"        tools:layout="@layout/ble_fragment"/>    <fragment        android:id="@+id/initFragment"        android:name="llc.arma.checklisttag.ui.init.InitFragment"        android:label="InitFragment"        tools:layout="@layout/init_fragment">        <action            android:id="@+id/action_initFragment_to_loginFragment"            app:destination="@id/loginFragment" />        <action            android:id="@+id/action_initFragment_to_scannerFragment"            app:destination="@id/scannerFragment" />        <action            android:id="@+id/action_initFragment_to_tagsFragment"            app:destination="@id/tagsFragment"            app:popUpTo="@id/tagsFragment"            app:popUpToInclusive="true" />    </fragment>    <fragment        android:id="@+id/scannerFragment"        android:name="llc.arma.checklisttag.ui.scanner.ScannerFragment"        android:label="ScannerFragment"        tools:layout="@layout/scanner_fragment"/>    <dialog        android:id="@+id/addNfcObjectDialog"        android:name="llc.arma.checklisttag.ui.add.AddNfcObjectDialog"        android:label="AddNfcObjectDialog"        tools:layout="@layout/add_nfc_object_fragment"/></navigation>
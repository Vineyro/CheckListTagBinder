<?xml version="1.0" encoding="utf-8"?><navigation xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:id="@+id/main_activity_graph"    app:startDestination="@id/loginFragment">    <fragment        android:id="@+id/tagsFragment"        android:name="llc.arma.checklisttag.ui.tag.TagsFragment"        android:label="TagsFragment"        tools:layout="@layout/tags_fragment">        <action            android:id="@+id/action_tagsFragment_to_scanFragment"            app:destination="@id/scanFragment" />        <action            android:id="@+id/action_tagsFragment_to_loginFragment2"            app:destination="@id/loginFragment"            app:launchSingleTop="true"            app:popUpTo="@id/tagsFragment"            app:popUpToInclusive="true" />        <action            android:id="@+id/action_tagsFragment_to_writeFragment"            app:destination="@id/writeFragment" />    </fragment>    <fragment        android:id="@+id/loginFragment"        android:name="llc.arma.checklisttag.ui.login.LoginFragment"        android:label="LoginFragment"        tools:layout="@layout/login_fragment">        <action            android:id="@+id/action_loginFragment_to_tagsFragment"            app:destination="@id/tagsFragment"            app:popUpTo="@id/loginFragment"            app:popUpToInclusive="true" />    </fragment>    <dialog        android:id="@+id/scanFragment"        android:name="llc.arma.checklisttag.ui.scan.ScanFragment"        android:label="ScanFragment"        tools:layout="@layout/scan_fragment"/>    <dialog        android:id="@+id/writeFragment"        android:name="llc.arma.checklisttag.ui.write.WriteFragment"        android:label="WriteFragment"        tools:layout="@layout/write_fragment"/></navigation>